{
  "manifest_version": 3,
  "name": "Website Visit Counter",
  "version": "1.0",
  "description": "Displays the number of times you've visited websites.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
